---
layout: main.njk
---

<main id="vocabulary">
	<div class="row">
		<div class="col-md">
			<h1 class="title-Header">{{ 'title' | i18n }}</h1>
			<ul>
				{%- for item in metadata.index.graph -%}
					<li>
						<a href="/{{ page.lang }}/voc/{{ item.id | notation  }}">{{ item.id | notation }} : {{ item.dctitle | label("fr") }}</a> <small class="numConcepts"> - {{ item.inverse_inScheme.length }} Concepts</small>
					</li>
				{%- endfor-%}
			</ul>
			
		</div>
	</div>
</main>