---
layout: main.njk
---

{% set sortIndex = agentsHeader.graph | jsonSort("rdfs:label") %}

<main class="section_garance">
	<div class="row">
		<link href="{{ '/pagefind/pagefind-ui.css' | relative(page) }}" rel="stylesheet">
		<script src="{{ '/pagefind/pagefind-ui.js' | relative(page) }}"></script>
		<div id="search"></div>
		<script>
		    window.addEventListener('DOMContentLoaded', (event) => {
		        new PagefindUI({ element: "#search", showSubResults: true });
		    });
		</script>
	</div>
	<div class="row">
		<div class="col-md">

			Contenu Ã©ditorial

			<h1 class="title-Header" data-i18n="agents"></h1>			
			<ul>
				{%- for item in sortIndex -%}
					{% if item.id %}
						<li>
							<a href="{{ [ '/entities/agent/', item.id | localName ] | join | relative(page) }}">{{ item["rdfs:label"] | lang("fr") }}</a>
						</li>
					{% endif %}
				{%- endfor-%}
			</ul>			
		</div>
	</div>
</main>
