---
layout: main.njk
---

{% set sortIndex = agentsHeader.graph | jsonSort("rdfs:label") %}

<main class="section_garance">
	<div class="row">
		<div class="col-md">

			{{ content | safe }}

			<hr />

			<ul>
				{%- for item in sortIndex -%}
					{% if item.id %}
						<li>
							<a href="{{ [ '/entities/agent/', item.id | localName ] | join | relative(page) }}">{{ item["rdfs:label"] | lang("fr") }}</a>
						</li>
					{% endif %}
				{%- endfor-%}
			</ul>			
		</div>
	</div>
</main>
